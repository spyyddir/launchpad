<!DOCTYPE html>
<html>
<head>
  <title>Launchpad</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>

<% if notice %>
	<p class="notice">Notice: <%= notice %></p>
<% end %>
<% if alert %>
	<p class="alert">Alert: <%= alert %></p>
<% end %>

<%= yield %>

</body>
</html>
